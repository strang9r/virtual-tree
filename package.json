{
  "name": "@strang9r/virtual-tree",
  "author": "Beyond9r",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/virtual-tree.cjs",
  "module": "./dist/virtual-tree.js",
  "types": "./dist/virtual-tree.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/strang9r"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/virtual-tree.js",
      "require": "./dist/virtual-tree.cjs"
    },
    "./css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "tsc -b && vite build --mode lib",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-virtualized": "^9.22.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.7.7",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-virtualized": "^9.21.30",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.4"
  }
